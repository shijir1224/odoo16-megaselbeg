<odoo>
    
        <template id="assets_common_technic" name="Technic's assets common" inherit_id="web.assets_common">
            <xpath expr="." position="inside">
                <link rel="stylesheet" 
                    href="/mw_technic_equipment/static/src/css/base.css"/>
                <link rel="stylesheet" 
                    href="/mw_technic_equipment/static/src/css/odometer-theme-car.css"/>
                <script type="text/javascript" src="mw_technic_equipment/static/src/js/odometer.js"></script>
                
                <script type="text/javascript" 
                    src="/mw_technic_equipment/static/src/libs/networkgraph.js"></script>
                <script type="text/javascript" 
                    src="/mw_technic_equipment/static/src/libs/treemap.js"></script>
                <link rel="stylesheet" 
                    href="/mw_technic_equipment/static/src/css/bootstrap-year-calendar.css"/>
            </xpath>
        </template>
        <template id="assets_backend_technic" name="Technic's assets backend"   
            inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/mw_technic_equipment/static/src/libs/bootstrap-year-calendar.js"></script>
                <script type="text/javascript" src="/mw_technic_equipment/static/src/js/widget.js"></script>
            </xpath>
        </template>
    
</odoo>
