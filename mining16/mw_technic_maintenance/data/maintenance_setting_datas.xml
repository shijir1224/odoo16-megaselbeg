			<field name="code">0203</field>
			<field name="description">...</field>
		</record>
		<record id="damage_reason07" model="maintenance.damaged.reason">
			<field name="name">Засварын чанар</field>
			<field name="code">0405</field>
			<field name="description">Боолт, гайка дутуу чангалж гаргах, буруу угсрах гм</field>
		</record>
		<record id="damage_reason08" model="maintenance.damaged.reason">
			<field name="name">Бусад</field>
			<field name="code">0501</field>
			<field name="description">Бусдын буруутай үйл ажиллагаанаас хамаарсан гм</field>
		</record>

		<!-- PM s -->
		<record id="type_01" model="maintenance.type">
			<field name="name">PM 1000</field>
			<field name="is_pm">True</field>
