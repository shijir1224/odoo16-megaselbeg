		</record>
		<record id="damage_reason04" model="maintenance.damaged.reason">
			<field name="name">Эд ангийн хугацаа дууссан</field>
			<field name="code">0102</field>
			<field name="description">Ажиллах цаг, норм нь дуусч хугацаандаа элэгдэж дууссан</field>
		</record>
		<record id="damage_reason05" model="maintenance.damaged.reason">
