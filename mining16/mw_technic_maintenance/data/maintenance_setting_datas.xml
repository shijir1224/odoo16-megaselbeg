		</record>
		<record id="type_02" model="maintenance.type">
			<field name="name">PM 2000</field>
			<field name="is_pm">True</field>
			<field name="color">#0AFF12</field>
		</record>
		<record id="type_03" model="maintenance.type">
			<field name="name">PM 250</field>
			<field name="is_pm">True</field>
			<field name="color">#FF3BFF</field>
		</record>
		<record id="type_04" model="maintenance.type">
			<field name="name">PM 500</field>
			<field name="is_pm">True</field>
			<field name="color">#FF2B0A</field>
		</record>
	
