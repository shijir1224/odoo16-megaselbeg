					<!-- DONUT -->
					<tr>
						<td  style="width:50%;height:200px">
							<div id="amount_donut_chart" class="chart_div_border">
							<b>12 months</b></div>
						</td>
						<td style="width:50%;height:200px">
							<div id="qty_donut_chart" class="chart_div_border">
							<b>12 months qty</b></div>
						</td>
					</tr>
				</table>
			</table>
		</div>
	</t>

