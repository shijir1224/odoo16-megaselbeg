<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_camp_room_tree" model="ir.ui.view">
        <field name="name">Camp Room tree</field>
        <field name="model">camp.room</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Check List">
                <field name="name" optional="show" />
                <field name="block_id" optional="show" />
            </tree>
        </field>
    </record>


    <record id="view_camp_room_form" model="ir.ui.view">
        <field name="name">Camp Room Form</field>
        <field name="model">camp.room</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="block_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_camp_room" model="ir.actions.act_window">
        <field name="name">Camp Room </field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">camp.room</field>
        <field name="view_mode">tree,form</field>
        <field name="context"></field>
    </record>

    <record id="view_camp_room_type_tree" model="ir.ui.view">
        <field name="name">Camp Room tree</field>
        <field name="model">camp.room.type</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Check List">
                <field name="name" optional="show" />
                <field name="price_unit" optional="show" />
            </tree>
        </field>
    </record>


    <record id="view_camp_room_type_form" model="ir.ui.view">
        <field name="name">Camp Room Form</field>
        <field name="model">camp.room.type</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="price_unit" />
                        <field name="revenue_account_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_camp_room_type" model="ir.actions.act_window">
        <field name="name">Camp Room </field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">camp.room.type</field>
        <field name="view_mode">tree,form</field>
        <field name="context"></field>
    </record>

    <record id="view_camp_department_tree" model="ir.ui.view">
        <field name="name">Camp department tree</field>
        <field name="model">camp.department</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Check List">
                <field name="name" optional="show" />
            </tree>
        </field>
    </record>


    <record id="view_camp_department_form" model="ir.ui.view">
        <field name="name">Camp department Form</field>
        <field name="model">camp.department</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_camp_department" model="ir.actions.act_window">
        <field name="name">Camp хэлтэс </field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">camp.department</field>
        <field name="view_mode">tree,form</field>
        <field name="context"></field>
    </record>
</odoo>